{
  "name": "cascader-plus",
  "version": "1.0.1",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://jeasonsun.github.io/cascader-plus/",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeasonSun/cascader-plus"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "@ant-design/icons": "*",
    "antd": "*",
    "rc-trigger": "*",
    "react": "*"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "rc-overflow": "^1.2.2",
    "rc-trigger": "^5.2.10",
    "react": "^16.12.0",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "@ant-design/icons": "^4.3.0",
    "antd": "^4.16.12",
    "dumi": "^1.0.16",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
